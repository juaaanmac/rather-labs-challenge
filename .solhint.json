{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "compiler-version": [
      "error",
      "0.8.19"
    ],
    "avoid-throw": false,
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "func-visibility": ["warn",{"ignoreConstructors":true}],
    "quotes": ["error","single"]
  }
}